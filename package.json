{
    "name": "pumper-diary-app",
    "version": "0.0.1",
    "author": "Stefan Braune <stefan.braune@gmail.com>",
    "private": true,
    "scripts": {
        "clean": "ionic-app-scripts clean",
        "build": "ionic-app-scripts build",
        "lint": "ionic-app-scripts lint",
        "ionic:build": "ionic-app-scripts build",
        "ionic:serve": "ionic-app-scripts serve",
        "build:android": "ionic cordova build --release android",
        "start:android": "ionic cordova run android",
        "start:adb": "ionic cordova emulate android -lcs -- -d",
        "start:web": "npm run ionic:serve",
        "start": "npm run start:web",
        "watch:android": "ionic cordova run android -lcs -- -d",
        "gen:keystore": "keytool -genkey -v -keystore ./release-key.keystore -alias zdevelopment.release -keyalg RSA -keysize 2048 -validity 10000",
        "sign:apk": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore ./release-key.keystore ./platforms/android/build/outputs/apk/android-release-unsigned.apk zdevelopment.release",
        "align:apk": "rimraf ./platforms/android/build/outputs/apk/android.apk && zipalign -v 4 ./platforms/android/build/outputs/apk/android-release-unsigned.apk ./platforms/android/build/outputs/apk/android.apk",
        "install:apk": "adb install -r ./platforms/android/build/outputs/apk/android.apk",
        "build:release": "npm run build:android && npm run sign:apk && npm run align:apk",
        "install:release": "npm run build:android && npm run sign:apk && npm run align:apk && npm run install:apk"
    },
    "dependencies": {
        "@angular/common": "4.1.3",
        "@angular/compiler": "4.1.3",
        "@angular/compiler-cli": "4.1.3",
        "@angular/core": "4.1.3",
        "@angular/forms": "4.1.3",
        "@angular/http": "4.1.3",
        "@angular/platform-browser": "4.1.3",
        "@angular/platform-browser-dynamic": "4.1.3",
        "@ionic-native/background-mode": "^4.2.1",
        "@ionic-native/core": "^3.12.1",
        "@ionic-native/insomnia": "^4.2.1",
        "@ionic-native/local-notifications": "^4.2.1",
        "@ionic-native/native-audio": "^3.12.1",
        "@ionic-native/splash-screen": "^3.12.1",
        "@ionic-native/status-bar": "^3.12.1",
        "@ionic-native/vibration": "^3.12.1",
        "@ionic/storage": "2.0.1",
        "@ngx-translate/core": "^6.0.1",
        "@ngx-translate/http-loader": "0.0.3",
        "@types/node": "^8.0.24",
        "cordova-android": "^6.2.3",
        "cordova-plugin-app-event": "file:node_modules\\cordova-plugin-app-event",
        "cordova-plugin-background-mode": "^0.7.2",
        "cordova-plugin-console": "^1.0.5",
        "cordova-plugin-device": "^1.1.4",
        "cordova-plugin-insomnia": "^4.3.0",
        "cordova-plugin-nativeaudio": "^3.0.9",
        "cordova-plugin-splashscreen": "^4.0.3",
        "cordova-plugin-sqlite-2": "^1.0.4",
        "cordova-plugin-statusbar": "^2.2.2",
        "cordova-plugin-vibration": "^2.1.5",
        "cordova-plugin-whitelist": "^1.3.1",
        "de.appplant.cordova.plugin.local-notification": "^0.8.5",
        "ionic-angular": "3.6.0",
        "ionic-plugin-keyboard": "^2.2.1",
        "ionicons": "3.0.0",
        "pouchdb": "6.2.0",
        "pouchdb-adapter-cordova-sqlite": "^2.0.2",
        "pouchdb-adapter-websql-core": "6.2.0",
        "pouchdb-browser": "6.2.0",
        "pouchdb-core": "6.2.0",
        "pouchdb-find": "^6.2.0",
        "rxjs": "5.4.0",
